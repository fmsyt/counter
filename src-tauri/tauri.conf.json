{
  "productName": "counter",
  "version": "0.1.0",
  "identifier": "dev.yotta-rc.counter",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "counter",
        "width": 400,
        "height": 300
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "windows": {
      "allowDowngrades": true,
      "certificateThumbprint": "1B1A97B87C6059CB8B58A007D1FFDC00D4F61811",
      "digestAlgorithm": "sha256",
      "nsis": null,
      "signCommand": null,
      "timestampUrl": "http://timestamp.comodoca.com",
      "tsp": false,
      "webviewInstallMode": {
        "silent": true,
        "type": "downloadBootstrapper"
      },
      "wix": null
    }
  }
